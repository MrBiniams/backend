{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "user"
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "isPhoneVerified": {
      "type": "boolean",
      "default": false
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "required": true,
      "default": "male"
    },
    "userSettings": {
      "type": "json",
      "default": {
        "emailNotifications": {
          "title": "Email Notifications",
          "description": "Receive email notifications for your bookings and other activities",
          "value": true
        },
        "smsNotifications": {
          "title": "SMS Notifications",
          "description": "Receive SMS notifications for your bookings and other activities",
          "value": true
        },
        "pushNotifications": {
          "title": "Push Notifications",
          "description": "Receive push notifications for your bookings and other activities",
          "value": true
        }
      }
    }
  }
}
