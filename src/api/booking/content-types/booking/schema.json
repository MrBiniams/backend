{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": "Parking bookings"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "plateNumber": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "required": true
    },
    "slot": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::slot.slot"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled",
        "completed"
      ],
      "default": "pending",
      "required": true
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "telebirr",
        "cbe-birr"
      ],
      "required": true
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "failed"
      ],
      "default": "pending",
      "required": true
    }
  }
} 