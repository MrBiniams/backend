{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location",
    "description": "Parking locations"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "component",
      "repeatable": false,
      "component": "common.coordinates",
      "required": true
    },
    "slots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slot.slot",
      "mappedBy": "location"
    }
  }
} 