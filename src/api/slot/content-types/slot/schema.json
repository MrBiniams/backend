{
  "kind": "collectionType",
  "collectionName": "slots",
  "info": {
    "singularName": "slot",
    "pluralName": "slots",
    "displayName": "Slot",
    "description": "Parking slots"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slotStatus": {
      "type": "enumeration",
      "enum": [
        "available",
        "occupied",
        "reserved",
        "maintenance"
      ],
      "required": true,
      "default": "available"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "standard",
        "premium",
        "disabled",
        "electric"
      ],
      "required": true,
      "default": "standard"
    },
    "coordinates": {
      "type": "component",
      "repeatable": false,
      "component": "common.coordinates",
      "required": true
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "slots"
    },
    "currentBooking": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::booking.booking"
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "features": {
      "type": "json"
    }
  }
}
